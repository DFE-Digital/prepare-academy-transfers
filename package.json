{
  "dependencies": {
    "axe-core": "^4.4.1",
    "cypress-axe": "^0.14.0"
  },
  "devDependencies": {
    "cypress": "^9.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.cy.js": [
      "npm run precommit"
    ]
  },  
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .cy.js,.jsx,.ts,.tsx --ignore-path .gitignore"
  }
}
